{
  "version": "0.2.36",
  "name": "embeding-clustering",
  "displayName": "Dataset Clustering",
  "description": "Tab for clustering and visualization of embeddings",
  "scope": "public",
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Application",
    "Application Type": "Data Management",
    "Libraries": ["TensorFlow", "Torch"],
    "License": "Apache 2.0"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/clustering-tab.git",
    "gitTag": "0.2.36"
  },
  "components": {
    "panels": [
      {
        "name": "tensorboard",
        "supportedSlots": [
          {
            "type": "datasetBrowser",
            "configuration": {
              "layout": {
                "rightBarView": "thumbnails"
              }
            }
          }
        ],
        "conditions": {}
      }
    ],
    "toolbars": [
      {
        "displayName": "Clustering",
        "icon": "",
        "invoke": {
          "type": "panel",
          "namespace": "tensorboard"
        },
        "location": "datasetBrowserTab"
      }
    ],
    "modules": [
      {
        "name": "tensorboard",
        "entryPoint": "main.py",
        "className": "Runner",
        "initInputs": [],
        "config": {
          "isCustomServer": true
        },
        "functions": [
          {
            "name": "run",
            "output": [],
            "input": []
          }
        ]
      }
    ],
    "services": [
      {
        "name": "tensorboard-runners",
        "moduleName": "tensorboard",
        "panelNames": ["tensorboard"],
        "initParams": {},
        "botUserName": "tab-apps@dataloop.ai",
        "runtime": {
          "podType": "highmem-l",
          "numReplicas": 1,
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/cpu/dataset-clustering:0.6.8",
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 1,
            "maxReplicas": 1,
            "queueLength": 1
          },
          "preemptible": false,
          "executionTimeout": 3600,
          "drainTime": 600,
          "onReset": "failed",
          "runExecutionAsProcess": false
        },
        "maxAttempts": 3
      }
    ]
  }
}
